<h1>Home</h1>

<button class="button blue-back" type="button" id="button-load">Load Programming Languages</button>

<div class="box" id="programming-langues-content">

</div>

<script>
    document.querySelector("#button-load").addEventListener("click", async function () {
        var response = await fetch("programming-languages");
        var languages = await response.text();

        document.querySelector("#programming-langues-content").innerHTML = languages;
    });
</script>

